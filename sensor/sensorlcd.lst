....................    delay_cycles(1); 
003F:  NOP
....................    lcd_output_enable(1); 
0040:  BSF    08.0
....................    delay_us(2); 
0041:  GOTO   042
....................    lcd_output_enable(0); 
0042:  BCF    08.0
0043:  RETURN
.................... } 
